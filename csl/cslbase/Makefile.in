@cygwin_TRUE@	objdump -p csl.exe | grep "DLL"
@cygwin_TRUE@	objdump -p bootstrapreduce.exe | grep "DLL"
@cygwin_TRUE@	objdump -p reduce.exe | grep "DLL"
install-data-local: $(image_files)
	@$(NORMAL_INSTALL)
	$(mkinstalldirs) $(DESTDIR)$(bindir)
	@list='$(image_files)'; for p in $$list; do \
	  if test -f $$p; then \
	    f=`echo "$$p" | sed 's,^.*/,,'`; \
	   echo " $(INSTALL_PROGRAM_ENV) $(binPROGRAMS_INSTALL) $$p $(DESTDIR)$(bindir)/$$f"; \
	   $(INSTALL_PROGRAM_ENV) $(binPROGRAMS_INSTALL) $$p $(DESTDIR)$(bindir)/$$f || exit 1; \
	  else :; fi; \
	done

@windows_TRUE@install-exec-local: $(console_binaries)
@windows_TRUE@	@$(NORMAL_INSTALL)
